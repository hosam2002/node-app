        .navbar__button {
        display: inline-block;
        height: 2.5rem;
        width: 2.5rem;
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
        }
        .navbar__checkbox {
        display: none;
        }
        .navbar__menu {
        width: 95%;
        height: 70vh;
        border-radius: 5px;
        background-color: #E0F2FE;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        transition: all 0.4s;
        display: none;
        }
        .navbar__icon {
        display: inline-block;
        width: 2.5rem;
        background-color: #0284C7;
        border-radius: 5px;
        position: relative;
        }
        .navbar__list {
        width: 100%;
        height: 100%;
        padding: 2.5rem 0;
        display: flex;
        justify-content: flex-start;
        align-items: center;
        flex-direction: column;
        gap: 1.5rem;
        }

        .navbar__icon, .navbar__icon::after, .navbar__icon::before {
        height: 0.3rem;
        }
        .navbar__icon::after, .navbar__icon::before {
        content: "";
        width: 2.5rem;
        border-radius: 5px;
        position: absolute;
        background-color: #0284C7;
        }
        .navbar__icon::after {
        top: 9px;
        }
        .navbar__icon::before {
        top: -9px;
        }

        .navbar__item {
        width: 80%;
        height: 5rem;
        border-bottom: 3px solid #0284C7;
        display: flex;
        justify-content: flex-start;
        align-items: center;
        font-size: 2rem;
        color: #0284C7;
        }
        .navbar__item a {
        text-decoration: none;
        color: inherit;
        display: inline-block;
        }

        .navbar__checkbox:checked ~ #menu {
        display: block;
        }

        .navbar {
        display: none;
        }
        @media (max-width: 37.5em) {
        .navbar {
            display: block;
        }
        }

        .navbar__checkbox:checked + .navbar__button .navbar__icon {
        background-color: transparent;
        }
        .navbar__checkbox:checked + .navbar__button .navbar__icon::after {
        transform: rotate(135deg);
        top: 0;
        }
        .navbar__checkbox:checked + .navbar__button .navbar__icon::before {
        transform: rotate(-135deg);
        top: 0;
        }

        main {
        width: 80%;
        padding: 2.5rem 0;
        margin: 0 auto;
        }
