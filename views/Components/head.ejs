        .i-nav {
        width: 3rem;
        height: 3px;
        border-radius: 5px;
        background-color: #FFFFFF;
        position: relative;
        }
        .i-nav::before, .i-nav::after {
        width: 3rem;
        height: 3px;
        border-radius: 5px;
        background-color: #FFFFFF;
        display: inline-block;
        }
        .i-nav::after, .i-nav::before {
        content: "";
        left: 0;
        position: absolute;
        transition: all 0.2s;
        }
        .i-nav::after {
        top: -10px;
        }
        .i-nav::before {
