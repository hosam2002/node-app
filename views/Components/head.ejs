        .nav {
        width: 70%;
        height: 100%;
        display: flex;
        flex-direction: row;
        justify-content: flex-end;
        align-items: center;
        gap: 1.5rem;
        display: none;
        }
        .nav__button {
        display: block;
        width: 3.5rem;
        height: 3.5rem;
        border-radius: 5px;
        order: 1;
        z-index: 600;
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
        }
        .nav__button > span {
        width: 3rem;
        height: 3px;
        border-radius: 5px;
        background-color: #FFFFFF;
        position: relative;
        }
        .nav__button > span::before, .nav__button > span::after {
        width: 3rem;
        height: 3px;
        border-radius: 5px;
        background-color: #FFFFFF;
        display: inline-block;
        }
        .nav__button > span::after, .nav__button > span::before {
        content: "";
        left: 0;
        position: absolute;
        transition: all 0.2s;
        }
        .nav__button > span::after {
        top: -10px;
        }
        .nav__button > span::before {
        top: 10px;
        }
        .nav__checkbox {
        display: none;
        }
        .nav__checkbox:checked ~ #nav__menu {
        margin-top: 0;
        }
        .nav__checkbox:checked ~ label > span {
        background-color: transparent;
        }
        .nav__checkbox:checked ~ label > span::after {
        transform: rotate(135deg);
        top: 0;
        }
        .nav__checkbox:checked ~ label > span::before {
        transform: rotate(-135deg);
        top: 0;
        }
        .nav__menu {
        width: 80%;
        height: 25rem;
        border-radius: 3px;
        margin-top: -100rem;
        transition: margin 0.6s;
        background-color: #1E293B;
        z-index: 500;
        position: absolute;
        top: 35%;
        left: 50%;
        transform: translate(-50%, -50%);
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
        }
        .nav__list {
        width: 100%;
        list-style: none;
        list-style-position: inside;
        text-transform: uppercase;
        font-size: 1.4rem;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        }
        .nav__list--item {
        display: inline-block;
        width: 95%;
        color: #FFFFFF;
        padding: 1rem;
        text-align: center;
        transition: background-color 0.3s;
        }
        .nav__list--item:nth-child(1) {
        background-color: #0F172A;
        }
        .nav__list--item:not(:last-child) {
        margin-bottom: 2.5rem;
        }
        .nav__list--item a {
        text-decoration: none;
        color: inherit;
        }
        .nav__list--item:nth-child(2):hover {
        background-color: #0F172A;
        }
        .nav__list--item:nth-child(3):hover {
        background-color: #0F172A;
        }

        /* Structure

            _ nav tag with nav class {

                _ radio with nav__radio class
                _ label for the input with nav__button class

                _ div width nav__menu class {

                    _ list with nav__list class {

                        _ List item with nav__list--item class
                    }
                }
            }
        */
