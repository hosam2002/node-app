        @media (max-width: 37.5em) {
        .search {
            display: none;
        }
        }
        .search__input {
        width: 20rem;
        height: 3rem;
        border-radius: 4px;
        padding: 0 1rem;
        font-size: 1.2rem;
        transition: width 0.4s;
